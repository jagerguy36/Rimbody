		      <li Class="Maux36.Rimbody.ModExtensionRimbodyTarget">
		      	  <Type>Building</Type>
		      	  <Category>Strength</Category>
		      	  <workouts>
		      	  	<li>
						<name>bench press</name>
						<reportString>benchpress_report</reportString>
						<cardio>0.4</cardio>
						<strength>2.0</strength>
						<animationType>melee</animationType>
						<playSound>true</playSound>
						<pawnDirection>faceSame</pawnDirection>
						<!-- shoulders, chest, biceps, triceps, back, core, glutes, quads, hams   -->
