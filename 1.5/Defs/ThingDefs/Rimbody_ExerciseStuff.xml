<?xml version="1.0" encoding="utf-8" ?>
<Defs>

	<ThingDef Name="ThingBase_RimbodyWorkout" Abstract="True">
		<thingClass>Maux36.Rimbody.Thing_WorkoutAnimated</thingClass>
    	<category>Item</category>
		<drawerType>MapMeshOnly</drawerType>
		<resourceReadoutPriority>Middle</resourceReadoutPriority>
		<useHitPoints>true</useHitPoints>
		<selectable>true</selectable>
		<altitudeLayer>Item</altitudeLayer>
    	<stackLimit>1</stackLimit>
		<statBases>
			<Beauty>-4</Beauty>
		</statBases>
		<comps>
			<li Class="CompProperties_Forbiddable" />
		</comps>
		<alwaysHaulable>true</alwaysHaulable>
    	<drawGUIOverlay>true</drawGUIOverlay>
    	<rotatable>false</rotatable>
		<pathCost>14</pathCost>
    	<allowedArchonexusCount>-1</allowedArchonexusCount>
    	<generateCommonality>0.0</generateCommonality>
	</ThingDef>

	<ThingDef ParentName="ThingBase_RimbodyWorkout">
		<defName>Rimbody_Dumbbell</defName>
		<label>dumbbell</label>
		<description>Free weights for a variety of exercises targeting different muscle groups.</description>
		<stackLimit>1</stackLimit>
		<graphicData>
			<texPath>Things/Item/Rimbody_things/AgRipnus/Dumbbells/Dumbbells</texPath>
			<graphicClass>Graphic_Single</graphicClass>
		</graphicData>
		<thingCategories>
			<li>Rimbody_Workout_equipment</li>
		</thingCategories>
		<techLevel>Industrial</techLevel>
		<statBases>
			<MaxHitPoints>150</MaxHitPoints>
			<Flammability>0.4</Flammability>
			<DeteriorationRate>0</DeteriorationRate>
			<Beauty>0</Beauty>
			<Mass>20</Mass>
		</statBases>
		<altitudeLayer>Item</altitudeLayer>
		<tickerType>Never</tickerType>
		<tradeability>Sellable</tradeability>
		<modExtensions>
			<li Class="Maux36.Rimbody.ModExtensionRimbodyTarget">
				<Type>Item</Type>
				<Category>Strength</Category>
				<rimbodyBuildingpartGraphics>
					<li>
						<texPath>Things/Item/Rimbody_things/AgRipnus/Dumbbells/Dumbbells_anim</texPath>
						<graphicClass>Graphic_Single</graphicClass>
					</li>
				</rimbodyBuildingpartGraphics>
				<workouts>
					<li>
						<name>bicepcurl_name</name>
						<reportString>dumbbell_bicepcurl_report</reportString>
						<cardio>0.4</cardio>
						<strength>2.0</strength>
						<animationType>item</animationType>
						<useBench>true</useBench>
						<movingpartAnimOffset>
							<south>(+0.33,0,-0.2)</south>
						</movingpartAnimOffset>
						<movingpartAnimPeak>
							<south>(0,0,0.3)</south>
						</movingpartAnimPeak>
						<!-- shoulders, chest, biceps, triceps, back, core, glutes, quads, hams   -->
						<strengthParts>
							<li>0</li>
							<li>0</li>
							<li>3</li>
							<li>0</li>
							<li>0</li>
							<li>0</li>
							<li>0</li>
							<li>0</li>
							<li>0</li>
						</strengthParts>
					</li>
					<li>
						<name>overheadshoulderpress_name</name>
						<reportString>dumbbell_overheadshoulderpress_report</reportString>
						<cardio>0.4</cardio>
						<strength>2.0</strength>
						<animationType>itemBoth</animationType>
						<useBench>true</useBench>
						<movingpartAnimOffset>
							<south>(0.4,0,0.3)</south>
						</movingpartAnimOffset>
						<movingpartAnimPeak>
							<south>(-0.05,0,0.5)</south>
						</movingpartAnimPeak>
						<strengthParts>
							<li>3</li>
							<li>0</li>
							<li>0</li>
							<li>0.5</li>
							<li>0</li>
							<li>0</li>
							<li>0</li>
							<li>0</li>
							<li>0</li>
						</strengthParts>
					</li>
				</workouts>
		    </li>
		</modExtensions>
	</ThingDef>

	<ThingDef ParentName="ThingBase_RimbodyWorkout">
		<defName>Rimbody_Haltere</defName>
		<label>haltere</label>
		<description>Handheld weights for strengthening and toning muscles through a range of exercises.</description>
		<stackLimit>2</stackLimit>
