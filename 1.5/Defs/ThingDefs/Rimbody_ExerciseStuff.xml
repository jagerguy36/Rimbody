<?xml version="1.0" encoding="utf-8" ?>
<Defs>

	<ThingDef Name="ThingBase_RimbodyWorkout" Abstract="True">
		<thingClass>Maux36.Rimbody.Thing_WorkoutAnimated</thingClass>
    	<category>Item</category>
		<drawerType>MapMeshOnly</drawerType>
		<resourceReadoutPriority>Middle</resourceReadoutPriority>
		<useHitPoints>true</useHitPoints>
		<selectable>true</selectable>
		<altitudeLayer>Item</altitudeLayer>
    	<stackLimit>1</stackLimit>
		<statBases>
			<Beauty>-4</Beauty>
		</statBases>
		<comps>
			<li Class="CompProperties_Forbiddable" />
		</comps>
		<alwaysHaulable>true</alwaysHaulable>
    	<drawGUIOverlay>true</drawGUIOverlay>
    	<rotatable>false</rotatable>
		<pathCost>14</pathCost>
    	<allowedArchonexusCount>-1</allowedArchonexusCount>
    	<generateCommonality>0.0</generateCommonality>
	</ThingDef>

	<ThingDef ParentName="ThingBase_RimbodyWorkout">
		<defName>Rimbody_Dumbbells</defName>
		<label>dumbbells</label>
		<description>Compact weights for versatile strength training. They help target specific muscle groups.</description>
		<stackLimit>1</stackLimit>
		<graphicData>
			<texPath>Things/Item/Rimbody_things/AgRipnus/Dumbbells/Dumbbells</texPath>
			<graphicClass>Graphic_Single</graphicClass>
		</graphicData>
		<thingCategories>
			<li>Rimbody_Workout_equipment</li>
		</thingCategories>
		<techLevel>Industrial</techLevel>
		<statBases>
			<WorkToMake>1200</WorkToMake>
			<MaxHitPoints>150</MaxHitPoints>
			<Flammability>0.4</Flammability>
			<DeteriorationRate>0</DeteriorationRate>
			<Beauty>0</Beauty>
			<Mass>20</Mass>
		</statBases>
		<altitudeLayer>Item</altitudeLayer>
		<tickerType>Never</tickerType>
		<tradeability>Sellable</tradeability>
		<costStuffCount>40</costStuffCount>
		<stuffCategories>
			<li>Metallic</li>
		</stuffCategories>
		<recipeMaker>
			<workSpeedStat>GeneralLaborSpeed</workSpeedStat>
			<effectWorking>ConstructMetal</effectWorking>
			<soundWorking>Recipe_Machining</soundWorking>
			<recipeUsers>
				<li>ElectricSmithy</li>
				<li>FueledSmithy</li>
			</recipeUsers>
			<unfinishedThingDef>UnfinishedWeapon</unfinishedThingDef>
			<defaultIngredientFilter>
				<categories>
					<li>Root</li>
				</categories>
			</defaultIngredientFilter>
			<researchPrerequisite>Rimbody_IndustrialEquipments</researchPrerequisite>
		</recipeMaker>
		<modExtensions>
			<li Class="Maux36.Rimbody.ModExtensionRimbodyTarget">
				<Type>Item</Type>
				<rimbodyTargetpartGraphics>
					<li>
						<texPath>Things/Item/Rimbody_things/AgRipnus/Dumbbells/DumbbellsAnim</texPath>
						<graphicClass>Graphic_Multi</graphicClass>
					</li>
				</rimbodyTargetpartGraphics>
				<workouts>
					<li>
						<name>bicepcurl_name</name>
						<reportString>dumbbells_bicepcurl_report</reportString>
						<Category>Strength</Category>
						<cardio>0.5</cardio>
						<strength>2.0</strength>
						<animationType>ItemEach</animationType>
						<spot>FlatBench</spot>
						<movingpartAnimOffset>
							<north>(0.33,0,-0.15)</north>
							<east>(0.45,0,-0.2)</east>
							<south>(0.33,0,-0.2)</south>
							<west>(-0.45,0,-0.2)</west>
						</movingpartAnimOffset>
						<movingpartAnimPeak>
							<north>(0,0,0.3)</north>
							<east>(-0.05,0,0.3)</east>
							<south>(0,0,0.3)</south>
							<west>(0.05,0,0.3)</west>
						</movingpartAnimPeak>
						<!-- shoulders, chest, biceps, triceps, back, core, glutes, quads, hams   -->
						<strengthParts>
							<li>0</li>
							<li>0</li>
							<li>3.25</li>
							<li>0</li>
							<li>0</li>
							<li>0</li>
							<li>0</li>
							<li>0</li>
							<li>0</li>
						</strengthParts>
					</li>
					<li>
						<name>overheadshoulderpress_name</name>
						<reportString>dumbbells_overheadshoulderpress_report</reportString>
						<Category>Strength</Category>
						<cardio>0.5</cardio>
						<strength>2.0</strength>
						<animationType>ItemBoth</animationType>
						<spot>FlatBench</spot>
						<movingpartAnimOffset>
							<north>(0.4,0,0.35)</north>
							<east>(0.15,0,0.15)</east>
							<south>(0.4,0,0.35)</south>
							<west>(-0.15,0,0.15)</west>
						</movingpartAnimOffset>
						<movingpartAnimPeak>
							<north>(-0.05,0,0.45)</north>
							<east>(-0.05,0,0.45)</east>
							<south>(-0.05,0,0.45)</south>
							<west>(0.05,0,0.45)</west>
						</movingpartAnimPeak>
						<strengthParts>
							<li>3</li>
							<li>0</li>
							<li>0</li>
							<li>0.5</li>
							<li>0</li>
							<li>0</li>
							<li>0</li>
							<li>0</li>
							<li>0</li>
						</strengthParts>
					</li>
				</workouts>
		    </li>
		</modExtensions>
	</ThingDef>

