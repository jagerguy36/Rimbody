							<li>0</li>
							<li>0</li>
							<li>0</li>
							<li>0</li>
						</strengthParts>
					</li>
					<li>
						<name>overheadshoulderpress_name</name>
						<reportString>halteres_overheadshoulderpress_report</reportString>
						<cardio>0.4</cardio>
						<strength>1.9</strength>
						<useBench>true</useBench>
						<movingpartAnimOffset>
							<south>(0.2,0,0.2)</south>
						</movingpartAnimOffset>
						<movingpartAnimPeak>
							<south>(0,0,0.3)</south>
						</movingpartAnimPeak>
						<strengthParts>
							<li>3</li>
