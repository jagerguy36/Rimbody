	<ThingDef ParentName = "FurnitureBase">
		<defName>Rimbody_WeightRack</defName>
		<thingClass>Building_Storage</thingClass>
		<label>weight rack</label>
		<description>A weight rack designed to organize and store weights, ensuring easy access and a clutter-free workout area. Place it near a workout machine to gain a boost in exercise efficiency.</description>
		<minTechLevelToBuild>Industrial</minTechLevelToBuild>
		<altitudeLayer>Building</altitudeLayer>
		<graphicData>
			<texPath>Things/Building/Rimbody_buildings/AgRipnus/Rack/Rack</texPath>
			<shaderType>CutoutComplex</shaderType>
			<drawSize>(1,1.37)</drawSize>
			<graphicClass>Graphic_Multi</graphicClass>
		</graphicData>
		<size>(1,1)</size>
		<fillPercent>0.6</fillPercent>
		<uiIconScale>1</uiIconScale>
		<rotatable>false</rotatable>
		<defaultPlacingRot>South</defaultPlacingRot>
		<passability>PassThroughOnly</passability>
		<pathCost>50</pathCost>
		<costStuffCount>70</costStuffCount>
		<stuffCategories>
			<li>Metallic</li>
			<li>Woody</li>
			<li>Stony</li>
		</stuffCategories>
		<leaveResourcesWhenKilled>true</leaveResourcesWhenKilled>
		<statBases>
			<MaxHitPoints>150</MaxHitPoints> 
			<Mass>40</Mass>
			<WorkToBuild>500</WorkToBuild>
			<Flammability>1.0</Flammability>
			<Beauty>0.5</Beauty>
		</statBases>
		<designationCategory>Furniture</designationCategory>
		<building>
			<preventDeteriorationOnTop>true</preventDeteriorationOnTop>
			<ignoreStoredThingsBeauty>true</ignoreStoredThingsBeauty>
			<maxItemsInCell>4</maxItemsInCell>
			<storageGroupTag>Shelf</storageGroupTag>
			<blueprintClass>Blueprint_Storage</blueprintClass>
			<fixedStorageSettings>
				<filter>
					<disallowNotEverStorable>true</disallowNotEverStorable>
					<categories>
						<li>Rimbody_Workout_equipment</li>
					</categories>
				</filter>
			</fixedStorageSettings>
			<defaultStorageSettings>
				<priority>Preferred</priority>
				<filter>
					<categories>
						<li>Rimbody_Workout_equipment</li>
					</categories>
				</filter>
			</defaultStorageSettings>
		</building>
		<inspectorTabs>
			<li>ITab_Storage</li>
		</inspectorTabs>
		<surfaceType>Item</surfaceType>
		<canOverlapZones>false</canOverlapZones>
		<researchPrerequisites>
			<li>Rimbody_IndustrialEquipments</li>
		</researchPrerequisites>
		<comps>
			<li Class="CompProperties_Facility">
				<statOffsets>
					<Rimbody_WorkoutEfficiency>0.05</Rimbody_WorkoutEfficiency>
				</statOffsets>
				<maxSimultaneous>1</maxSimultaneous>
			</li>
		</comps>
		<placeWorkers>
			<li>PlaceWorker_ShowFacilitiesConnections</li>
		</placeWorkers>
	</ThingDef>
	
